{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceRoot}/StickerApp/bin/Debug/netcoreapp1.1/StickerApp.dll",
            "args": [],
            "env": { 
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "cwd": "${workspaceRoot}/StickerApp",
            "stopAtEntry": false,
            "externalConsole": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}